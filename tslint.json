{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "curly": false,
    "no-unused-expression": false,
    "variable-name": [true, "check-format", "allow-leading-underscore"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}