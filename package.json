{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "babel-node": "babel-node -x '.js,.ts,.tsx' --env-name node",
    "build": "npm run webpack -- --mode production",
    "lint": "concurrently -n typescript,eslint \"npm run lint-typescript\" \"npm run lint-eslint\"",
    "lint-eslint": "eslint '**/*.{js,ts,tsx}'",
    "lint-typescript": "tsc",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "npm run babel-node -- node_modules/.bin/mocha 'src/**/*.test.ts'",
    "dev": "npm_config_loglevel=silent concurrently -k -n webpack-dev-server,lint,test-unit \"npm run webpack-dev-server\" \"npm run watch-lint\" \"npm run watch-test-unit\"",
    "watch-lint": "concurrently -k -n typescript,eslint \"npm run watch-lint-typescript\" \"npm run watch-lint-eslint\"",
    "watch-lint-eslint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint-eslint -- --fix",
    "watch-lint-typescript": "npm run lint-typescript -- --watch --preserveWatchOutput --pretty",
    "watch-test-unit": "onchange -ki 'src/**/*.js' -- npm run test-unit",
    "webpack": "npm run babel-node -- node_modules/.bin/webpack",
    "webpack-analyze": "FILE=`mktemp -t stats` && npm run -s webpack -- --mode production --profile --json > $FILE && npx webpack-bundle-analyzer $FILE dist/browser",
    "webpack-dev-server": "npm run babel-node -- node_modules/.bin/webpack-dev-server --mode development"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@frosas/eslint-config": "^1.1.0",
    "@sentry/browser": "^5.13.2",
    "@types/babel__core": "^7.1.6",
    "@types/chai": "^4.2.10",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/d3-scale": "^2.2.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/webpack-dev-server": "^3.10.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "d3-scale": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.0",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "regenerator-runtime": "^0.13.3",
    "source-map-loader": "^0.2.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
