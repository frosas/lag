{
  "name": "lag",
  "private": true,
  "scripts": {
    "build": "vite build",
    "lint": "concurrently -n typescript,eslint \"npm run lint-typescript\" \"npm run lint-eslint\"",
    "lint-eslint": "eslint . --max-warnings=0",
    "lint-typescript": "tsc",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "vitest run",
    "dev": "npm_config_loglevel=silent concurrently -k -n vite,lint,test-unit \"npm run dev-web-server\" \"npm run dev-lint\" \"npm run dev-test-unit\"",
    "dev-lint": "concurrently -k -n typescript,eslint \"npm run dev-lint-typescript\" \"npm run dev-lint-eslint\"",
    "dev-lint-eslint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint-eslint -- --fix",
    "dev-lint-typescript": "npm run lint-typescript -- --watch --preserveWatchOutput --pretty",
    "dev-test-unit": "vitest watch",
    "dev-web-server": "vite",
    "prepare": "husky"
  },
  "devDependencies": {
    "@frosas/eslint-config": "^9.2.0",
    "@sentry/browser": "^8.55.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/d3-scale": "^4.0.1",
    "@types/eslint": "^9.0.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "compression": "^1.7.2",
    "concurrently": "^9.0.0",
    "d3-scale": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eventemitter3": "^5.0.1",
    "husky": "^9.0.11",
    "onchange": "^7.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.4.5",
    "vite": "^7.2.7",
    "vitest": "^4.0.0"
  }
}
