{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "main": "src/node/webServer",
  "scripts": {
    "watch": "concurrently --kill-others --names webpack,lint,mocha,web-server \"npm run watch-webpack\" \"npm run watch-lint\" \"npm run watch-mocha\" \"npm run watch-web-server\"",
    "watch-webpack": "nodemon -q -e js,ts -w webpack.config.js -w src/build -w src/universal -x \"webpack --watch\"",
    "watch-lint": "concurrently -k -n eslint,tslint \"npm run watch-eslint\" \"npm run watch-tslint\"",
    "watch-eslint": "nodemon -q -w src -w test -x \"eslint . --fix\"",
    "watch-tslint": "nodemon -q -e ts -w src -x \"tslint 'src/**/*.ts' --fix || exit 1\"",
    "watch-mocha": "nodemon -q -w src -w test -x \"mocha 'test/**/*.js'\"",
    "watch-web-server": "nodemon -q -w src/server -w src/universal -x \"npm start\"",
    "start": "node -r dotenv/config .",
    "browser": "source .env && open http://localhost:$PORT"
  },
  "engines": {
    "node": "^8.3.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.1",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.28.7",
    "d3": "^4.11.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash-es": "^4.17.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "@types/d3-scale": "^1.0.10",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.27",
    "awesome-typescript-loader": "^3.4.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.1"
  }
}
