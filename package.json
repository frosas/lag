{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run webpack -- --mode production",
    "lint": "concurrently -n typescript,eslint \"npm run lint-typescript\" \"npm run lint-eslint\"",
    "lint-eslint": "eslint '**/*.{js,ts,tsx}'",
    "lint-typescript": "tsc --noEmit",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "mocha 'src/**/*.test.js'",
    "watch": "concurrently -k -n webpack,lint,test-unit,web-server \"npm run watch-webpack\" \"npm run watch-lint\" \"npm run watch-test-unit\" \"npm run web-server\"",
    "watch-lint": "concurrently -k -n typescript,eslint \"npm run watch-lint-typescript\" \"npm run watch-lint-eslint\"",
    "watch-lint-eslint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint-eslint -- --fix",
    "watch-lint-typescript": "npm run lint-typescript -- --watch --preserveWatchOutput --pretty",
    "watch-test-unit": "onchange -ki 'src/**/*.js' -- npm run test-unit",
    "watch-webpack": "npm run webpack -- --mode development --watch",
    "web-server": "http-server dist/browser",
    "webpack": "babel-node -x '.js,.ts,.tsx' node_modules/.bin/webpack"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@frosas/eslint-config": "^1.1.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/d3-scale": "^2.0.1",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-loader": "^8.0.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "d3-scale": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.0.1",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "source-map-loader": "^0.2.3",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.11"
  }
}
