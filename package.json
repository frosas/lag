{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "mocha 'src/**/*.test.js'",
    "watch": "concurrently --kill-others --names webpack,lint,mocha,web-server \"npm run watch-webpack\" \"npm run watch-lint\" \"npm run watch-mocha\" \"npm run web-server\"",
    "watch-lint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint -- --fix",
    "watch-mocha": "onchange -ki 'src/**/*.js' -- npm t",
    "watch-webpack": "onchange -ki webpack.config.js 'src/{build,universal}/**/*.{js,ts}' -- webpack --mode development --watch",
    "web-server": "http-server dist/browser"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "@frosas/eslint-config": "^1.1.0",
    "@types/d3-scale": "^2.0.1",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "d3-scale": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "lodash-es": "^4.17.11",
    "mocha": "^7.0.1",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
