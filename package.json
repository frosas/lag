{
  "name": "lag",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "npm_config_loglevel=silent concurrently -k -n vite,typecheck,lint,test-unit npm:dev-web-server npm:dev-typecheck npm:dev-lint npm:dev-test-unit",
    "dev-lint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint -- --fix",
    "dev-typecheck": "npm run typecheck -- --watch --preserveWatchOutput --pretty",
    "dev-test-unit": "vitest watch",
    "dev-web-server": "vite",
    "lint": "eslint . --max-warnings=0",
    "prepare": "husky",
    "typecheck": "tsc",
    "test": "concurrently -n lint,typecheck,unit-test,build npm:lint npm:typecheck npm:test-unit npm:build",
    "test-unit": "vitest run"
  },
  "devDependencies": {
    "@frosas/eslint-config": "^10.0.1",
    "@sentry/browser": "^10.0.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/d3-scale": "^4.0.1",
    "@types/eslint": "^9.0.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "compression": "^1.7.2",
    "concurrently": "^9.0.0",
    "d3-scale": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eventemitter3": "^5.0.1",
    "husky": "^9.0.11",
    "onchange": "^7.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.4.5",
    "vite": "^7.2.7",
    "vitest": "^4.0.0"
  }
}
