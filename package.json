{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "eslint": "eslint .",
    "lint": "concurrently -n eslint,tslint \"npm run eslint\" \"npm run tslint\"",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "mocha 'test/**/*.js'",
    "tslint": "tslint 'src/**/*.{ts,tsx}'",
    "watch": "concurrently --kill-others --names webpack,lint,mocha,web-server \"npm run watch-webpack\" \"npm run watch-lint\" \"npm run watch-mocha\" \"npm run web-server\"",
    "watch-webpack": "onchange -i webpack.config.js 'src/{build,universal}/**/*.{js,ts}' -- webpack --mode development --watch",
    "watch-lint": "concurrently -k -n eslint,tslint \"npm run watch-eslint\" \"npm run watch-tslint\"",
    "watch-eslint": "onchange -i '{src,test}/**/*.js' -- npm run eslint -- --fix",
    "watch-tslint": "onchange -i 'src/**/*.ts' -- npm run tslint -- --fix",
    "watch-mocha": "onchange -i '{src,test}/**/*.js' -- npm t",
    "web-server": "http-server dist/browser"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@types/d3-scale": "^2.0.1",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^3.4.2",
    "d3-scale": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "lodash-es": "^4.17.11",
    "mocha": "^5.2.0",
    "onchange": "^4.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.12.0",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@frosas/eslint-config": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "webpack-cli": "^3.3.10"
  }
}
