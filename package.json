{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "main": "src/server/http",
  "scripts": {
    "watch": "concurrently --kill-others --names \"webpack,eslint,mocha,web-server\" \"npm run watch-webpack\" \"npm run watch-eslint\" \"npm run watch-mocha\" \"npm run watch-web-server\"",
    "watch-webpack": "nodemon -q -w \"webpack.config.js\" -w \"src/build\" -w \"src/universal\" -x \"webpack --watch\"",
    "watch-eslint": "nodemon -q -w \"src\" -w \"test\" -x \"eslint . --fix\"",
    "watch-mocha": "nodemon -q -w \"src\" -w \"test\" -x \"mocha 'test/**/*.js'\"",
    "watch-web-server": "nodemon -q -w \"src/server\" -w \"src/universal\" -x \"npm start\"",
    "#": "Following scripts are used by Dokku",
    "postinstall": "bin/npm-postinstall",
    "start": "bin/server"
  },
  "engines": {
    "node": "^8.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017",
      "react"
    ]
  },
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.5.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "d3": "^4.7.0",
    "debug": "^2.6.1",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "lodash-es": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.5.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.4.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3"
  }
}
