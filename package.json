{
  "name": "lag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "babel-node": "babel-node -x '.js,.ts,.tsx' --env-name node",
    "build": "npm run webpack -- --mode production",
    "lint": "concurrently -n typescript,eslint \"npm run lint-typescript\" \"npm run lint-eslint\"",
    "lint-eslint": "eslint '**/*.{js,ts,tsx}'",
    "lint-typescript": "tsc",
    "test": "concurrently -n lint,unit-test,build \"npm run lint\" \"npm run test-unit\" \"npm run build\"",
    "test-unit": "npm run babel-node -- node_modules/.bin/mocha 'src/**/*.test.ts'",
    "dev": "npm_config_loglevel=silent concurrently -k -n webpack-dev-server,lint,test-unit \"npm run webpack-dev-server\" \"npm run dev-lint\" \"npm run dev-test-unit\"",
    "dev-lint": "concurrently -k -n typescript,eslint \"npm run dev-lint-typescript\" \"npm run dev-lint-eslint\"",
    "dev-lint-eslint": "onchange -ki --exclude-path .gitignore '**/*.{js,ts,tsx}' -- npm run lint-eslint -- --fix",
    "dev-lint-typescript": "npm run lint-typescript -- --watch --preserveWatchOutput --pretty",
    "dev-test-unit": "onchange -ki 'src/**/*.js' -- npm run test-unit",
    "prepare": "husky install",
    "webpack": "npm run babel-node -- node_modules/.bin/webpack",
    "webpack-analyze": "FILE=`mktemp -t stats` && npm run -s webpack -- --mode production --profile --json > $FILE && npx webpack-bundle-analyzer $FILE dist/browser",
    "webpack-dev-server": "npm run babel-node -- node_modules/.bin/webpack serve --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@frosas/eslint-config": "^3.0.0",
    "@sentry/browser": "^6.2.0",
    "@types/babel__core": "^7.1.12",
    "@types/chai": "^4.2.15",
    "@types/copy-webpack-plugin": "~6.4.0",
    "@types/d3-scale": "^3.2.2",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mocha": "^8.2.1",
    "@types/node": "^12.20.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.2.2",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.2",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.3",
    "d3-scale": "^3.2.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^5.1.3",
    "mini-css-extract-plugin": "^1.3.9",
    "mocha": "^8.3.0",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "source-map-loader": "^1.1.3",
    "typescript": "^4.2.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
